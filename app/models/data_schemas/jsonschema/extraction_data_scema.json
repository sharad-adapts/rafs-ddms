{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "SamplesAnalysisID": {
            "type": "string",
            "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-SamplesAnalysis:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        },
        "SamplesID": {
            "type": "string",
            "pattern": "^[\\w-.]+:master-data-SamplesID:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        },
        "WeightOfRockExtracted": {
            "type": "object",
            "properties": {
                "Value": {
                    "type": "number"
                },
                "UnitOfMeasure": {
                    "type": "string",
                    "pattern": "^[\\w-.]:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
            }
        },
        "WeightOfExtract": {
            "type": "object",
            "properties": {
                "Value": {
                    "type": "number"
                },
                "UnitOfMeasure": {
                    "type": "string",
                    "pattern": "^[\\w-.]:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
            }
        },
        "ExtractPpmByRockWeight": {
            "type": "object",
            "properties": {
                "Value": {
                    "type": "number"
                },
                "UnitOfMeasure": {
                    "type": "string",
                    "pattern": "^[\\w-.]:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
            }
        },
        "ExtractPctOfTotalOrganicCarbon": {
            "type": "object",
            "properties": {
                "Value": {
                    "type": "number"
                },
                "UnitOfMeasure": {
                    "type": "string",
                    "pattern": "^[\\w-.]:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
            }
        },
        "HydrocarbonPpmOfExtractByWeight": {
            "type": "object",
            "properties": {
                "Value": {
                    "type": "number"
                },
                "UnitOfMeasure": {
                    "type": "string",
                    "pattern": "^[\\w-.]:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
            }
        },
        "HydrocarbonsToTotalExtractRatio": {
            "type": "number"
        },
        "AlkanesToHydrocarbonsRatio": {
            "type": "number"
        }
    },
    "required": [
        "SamplesAnalysisID",
        "SamplesID"
    ]
}
