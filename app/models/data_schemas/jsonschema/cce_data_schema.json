{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "ConstantCompositionExpansionTestID": {
      "title": "Constant Composition Expansion Test ID",
      "type": "string",
      "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-ConstantCompositionExpansionTest:[\\w\\-\\.\\:\\%]+:[0-9]*$"
    },
    "TestNumber": {
      "title": "Test Number",
      "type": "string",
      "description": "A number for this test for purposes of e.g., tracking lab sequence."
    },
    "FluidSampleID": {
      "type": "string",
      "title": "Fluid Sample ID",
      "description": "Unique identifier of the Fluid Sample which constant composition expansion analysis is done on",
      "pattern": "^[\\w\\-\\.]+:master-data\\-\\-FluidSample:[\\w\\-\\.\\:\\%]+:[0-9]*$"
    },
    "TestTemperature": {
      "title": "Test Temperature",
      "description": "The temperature of this test.",
      "x-osdu-frame-of-reference": "UOM:temperature",
      "type": "object",
      "properties": {
        "Value": {
          "type": "number"
        },
        "UnitOfMeasure": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        }
      }
    },
    "SaturationPressure": {
      "title": "Saturation Pressure",
      "description": "The saturation (or bubble point) pressure measured in this test.",
      "x-osdu-frame-of-reference": "UOM:reciprocal_pressure",
      "type": "object",
      "properties": {
        "Value": {
          "type": "number"
        },
        "UnitOfMeasure": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        },
        "Type": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-PressureMeasurementType:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        }
      }
    },
    "ReservoirPressure": {
      "title": "Reservoir Pressure",
      "type": "object",
      "properties": {
        "Value": {
          "type": "number"
        },
        "UnitOfMeasure": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        }
      }
    },
    "ConstantCompositionExpansionTestSteps": {
      "name": "Constant Composition Expansion Test Step",
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "StepNumber": {
              "title": "Step Number",
              "type": "string",
              "description": "Internal step number which identifies the sequence of step in CCE measurements"
            },
            "StepPressure": {
              "title": "Step Pressure",
              "description": "The pressure applied during the step.",
              "x-osdu-frame-of-reference": "UOM:psi",
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                },
                "Type": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-PressureMeasurementType:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              }
            },
            "LiquidFraction": {
              "title": "Liquid Fraction",
              "description": "The fraction of liquid by volume for this test step. This is the volume of liquid divided...",
              "x-osdu-frame-of-reference": "UOM:%",
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              }
            },
            "OilDensity": {
              "title": "Oil Density",
              "description": "The density of the oil for this test step.",
              "x-osdu-frame-of-reference": "UOM:kg/m3",
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              }
            },
            "OilCompressibility": {
              "title": "Oil Compressibility",
              "description": "The compressibility of the oil for this test step.",
              "x-osdu-frame-of-reference": "UOM:psi",
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              }
            },
            "Oil Viscosity": {
              "value": "Oil Viscosity",
              "description": "The dynamic viscosity of the oil for this test step.",
              "x-osdu-frame-of-reference": "UOM:g/cm3",
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              }
            },
            "TotalVolume": {
              "title": "Total Volume",
              "description": "The total volume for this test step.",
              "x-osdu-frame-of-reference": "UOM:bbl/bbl",
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              }
            },
            "SaturationPressureLiquidVolume": {
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              }
            },
            "IndicatedPressureLiquidVolume": {
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              }
            },
            "RelativeVolumeRatio": {
              "value": "Relative Volume Ration",
              "description": "The measured relative volume ratio = measured volume/volume at Psat.",
              "x-osdu-frame-of-reference": "UOM:bbl/bbl",
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              }
            },
            "GasDensity": {
              "value": "Gas Density",
              "description": "The density of the gas for this test step.",
              "x-osdu-frame-of-reference": "UOM:kg/m3",
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              }
            },
            "GasZFactor": {
              "title": "GasZFactor",
              "type": "number",
              "description": "The Z factor of the gas for this test step."
            },
            "GasCompressibility": {
              "title": "Gas Compressibility",
              "description": "The compressibility of the gas for this test step.",
              "x-osdu-frame-of-reference": "UOM:psi",
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              }
            },
            "GasViscosity": {
              "title": "Gas Viscosity",
              "description": "The viscosity of the gas phase at this test step",
              "x-osdu-frame-of-reference": "UOM:cP",
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              }
            },
            "YFunction": {
              "title": "Y Function",
              "type": "number",
              "description": "The Y function at this test step. See  Standing, M.B.: Volumetric And Phase Behavior Of Oil Field Hydrocarbon Systems, Eighth Edition, SPE Richardson, Texas (1977)."
            },
            "FluidConditionType": {
              "title": "Fluid Condition Type",
              "type": "string",
              "description": "The fluid condition at this test step.",
              "enum": [
                "Current Reservoir Conditions",
                "Initial Reservoir Conditions",
                "Initial Saturation Conditions",
                "Stock Tank Conditions"
              ]
            },
            "PhasesPresentType": {
              "type": "string",
              "enum": [
                "Oil",
                "Solid",
                "Gas",
                "Oil and Gas"
              ]
            }
          }
        }
      ]
    }
  },
  "required": [
    "id"
  ]
}
