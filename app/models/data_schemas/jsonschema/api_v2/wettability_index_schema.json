{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "SamplesAnalysisID": {
            "type": "string",
            "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-SamplesAnalysis:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        },
        "SampleID": {
            "type": "string",
            "pattern": "^[\\w\\-\\.]+:master-data\\-\\-Sample:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        },
        "WettabilityIndexData": {
            "type": "object",
            "properties": {
                "CapillaryPressureAnalysisID": {
                    "type": "string",
                    "description": "OSDU ID for the Capillary Pressure Analysis referenced in this Wettability Analysis.",
                    "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-SamplesAnalysis:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                },
                "ForcedImbibedBrineVolume": {
                    "type": "object",
                    "description": "volume of forced imbibed brine, displacing water, expressed as fraction of pore volume. no confining pressure applied",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "ForcedImbibedOilVolume": {
                    "type": "object",
                    "description": "volume of forced imbibed oil, displacing water, expressed as fraction of pore volume. no confining pressure applied",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "Temperature": {
                    "type": "object",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "InitialBrineSaturation": {
                    "type": "object",
                    "description": "initial brine saturation expressed as a fraction of the pore volume",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "InitialOilSaturation": {
                    "type": "object",
                    "description": "initial oil saturation expressed as a fraction of the pore volume",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "SpontaneousImbibedBrineVolume": {
                    "type": "object",
                    "description": "volume of spontaneous imbibed brine, displacing oil, expressed as fraction of pore volume. no confining pressure applied",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "DisplacedOilVolume": {
                    "type": "object",
                    "description": "volume of oil displaced by brine, fraction percent of the pore volume",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "BrineImbibitionBrineSaturation": {
                    "type": "object",
                    "description": "volume of brine saturation during brine imbibition, fraction percent of the pore volume",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "BrineDisplacementOilSaturation": {
                    "type": "object",
                    "description": "volume of oil saturation during brine displacement, expressed in fraction of pore volume",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "SpontaneousImbibedOilVolume": {
                    "type": "object",
                    "description": "volume of spontaneously imbibed oil, displacing brine, expressed as fraction of pore volume. no confining pressure applied",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "OilImbibitionBrineSaturation": {
                    "type": "object",
                    "description": "volume of brine saturation during oil imbibition, expressed as fraction of pore volume",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "DisplacedBrineVolume": {
                    "type": "object",
                    "description": "volume of brine displaced by oil, fraction percent of the pore volume",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "DisplacementRatio": {
                    "type": "object",
                    "description": "volume of brine displaced by oil, fraction percent of the pore volume",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "OilImbibitionOilSaturation": {
                    "type": "object",
                    "description": "volume of oil saturation during oil imbibition, expressed as fraction of pore volume",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "WettabilityIndex": {
                    "type": "array",
                    "description": "Wettability index",
                    "items": {
                        "type": "object",
                        "properties": {
                            "Value": {
                                "type": "number"
                            },
                            "UnitOfMeasure": {
                                "type": "string",
                                "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                            },
                            "WettabilityIndexType": {
                                "type": "string",
                                "pattern": "^[\\w\\-\\.]+:reference-data--WettabilityIndexType:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                            }
                        },
                        "additionalProperties": false
                    }
                },
                "ConfiningPressure": {
                    "type": "object",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "DesaturationMethod": {
                    "type": "string",
                    "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-DesaturationMethod:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                },
                "DisplacingFluid": {
                    "type": "string",
                    "description": "Fluid introduced to a sample during wettability index testing, e.g. Mercury",
                    "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-DisplacingFluidType:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                },
                "DisplacedFluid": {
                    "type": "string",
                    "description": "Fluid produced or displaced from a sample during wettability index testing, e.g. Brine, Hexane, or LiveCrudeOil",
                    "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-DisplacedFluidType:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                },
                "FluidSystem": {
                    "type": "string",
                    "pattern": "^[\\w\\-\\.]+:reference-data--FluidSystemType:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                },
                "InitialWaterSaturation": {
                    "type": "object",
                    "description": "initial water saturation expressed as a fraction of the pore volume",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "InitialCapillaryPressure": {
                    "type": "object",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "ForcedWaterImbibition": {
                    "type": "object",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                },
                "ForcedOilImbibition": {
                    "type": "object",
                    "properties": {
                        "Value": {
                            "type": "number"
                        },
                        "UnitOfMeasure": {
                            "type": "string",
                            "pattern": "^[\\w\\-\\.]+:reference-data--UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                        }
                    },
                    "additionalProperties": false
                }
            },
            "additionalProperties": false
        }
    },
    "additionalProperties": false,
    "required": [
        "SampleID",
        "SamplesAnalysisID"
    ]
}
