                                "properties": {
                                    "Value": {
                                        "type": "number"
                                    },
                                    "UnitOfMeasure": {
                                        "type": "string",
                                        "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                                    }
                                },
                                "additionalProperties": false
                            },
                            "T2Mean": {
                                "type": "object",
                                "description": "Average of T2 relaxation time",
                                "properties": {
                                    "Value": {
                                        "type": "number"
                                    },
                                    "UnitOfMeasure": {
                                        "type": "string",
                                        "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                                    }
                                },
                                "additionalProperties": false
                            },
                            "Temperature": {
                                "type": "object",
                                "description": "Test temperature",
                                "properties": {
                                    "Value": {
                                        "type": "number"
                                    },
                                    "UnitOfMeasure": {
                                        "type": "string",
                                        "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                                    }
                                },
                                "additionalProperties": false
                            },
                            "PoreVolume": {
                                "type": "object",
                                "description": "Pore Volume",
                                "properties": {
                                    "Value": {
                                        "type": "number"
                                    },
                                    "UnitOfMeasure": {
                                        "type": "string",
                                        "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                                    }
                                },
                                "additionalProperties": false
                            },
                            "NetConfiningStress": {
                                "type": "object",
                                "description": "Net Confining Stress",
                                "properties": {
                                    "Value": {
                                        "type": "number"
                                    },
                                    "UnitOfMeasure": {
                                        "type": "string",
                                        "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
