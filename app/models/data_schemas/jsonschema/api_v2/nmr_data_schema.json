                                    "properties": {
                                        "Value": {
                                            "type": "number"
                                        },
                                        "UnitOfMeasure": {
                                            "type": "string",
                                            "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                                        }
                                    },
                                    "additionalProperties": false
                                },
                                "T2FullySaturated": {
                                    "type": "object",
                                    "description": "T2 (transverse or spin-spin relaxation) when the sample is fully saturated",
                                    "properties": {
                                        "Value": {
                                            "type": "number"
