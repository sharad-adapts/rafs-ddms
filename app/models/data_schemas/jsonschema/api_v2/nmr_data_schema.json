                                        "UnitOfMeasure": {
                                            "type": "string",
                                            "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                                        }
                                    },
                                    "additionalProperties": false
                                },
                                "T1CumulativeWaterSaturation": {
                                    "type": "object",
                                    "description": "Cumulative water saturation of the core plug based on T1",
                                    "properties": {
                                        "Value": {
                                            "type": "number"
                                        },
                                        "UnitOfMeasure": {
                                            "type": "string",
                                            "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                                        }
                                    },
                                    "additionalProperties": false
                                },
                                "T1IncrementalPorosity": {
                                    "type": "object",
                                    "description": "Incremental porosity of the core plug based on T1",
                                    "properties": {
                                        "Value": {
                                            "type": "number"
                                        },
                                        "UnitOfMeasure": {
                                            "type": "string",
                                            "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                                        }
                                    },
                                    "additionalProperties": false
                                },
                                "T1IncrementalWaterSaturation": {
                                    "type": "object",
                                    "description": "Incremental water saturation of the core plug based on T1",
                                    "properties": {
                                        "Value": {
                                            "type": "number"
                                        },
                                        "UnitOfMeasure": {
                                            "type": "string",
                                            "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
