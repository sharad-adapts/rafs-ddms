{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "SamplesAnalysisID": {
            "type": "string",
            "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-SamplesAnalysis:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        },
        "SampleID": {
            "type": "string",
            "pattern": "^[\\w\\-\\.]+:master-data\\-\\-Sample:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        },
        "NMRTest": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "TestCondition": {
                        "title": "Test Condition",
                        "description": "The sample condition at which the NMR test was conducted",
                        "type": "string",
                        "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-NMRTestCondition:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                    },
                    "NMRTestSteps": {
                        "title": "NMR Test Steps",
                        "description": "The dynamic core nmr test steps",
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "CumulativeWaterSaturation": {
                                    "type": "object",
                                    "description": "Cumulative water saturation",
                                    "properties": {
                                        "Value": {
                                            "type": "number"
                                        },
                                        "UnitOfMeasure": {
                                            "type": "string",
                                            "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                                        }
                                    },
                                    "additionalProperties": false
                                },
                                "CumulativeVolume": {
                                    "type": "object",
                                    "description": "Cumulative volume of fluid calculated based on amplitude measurement",
                                    "properties": {
                                        "Value": {
                                            "type": "number"
                                        },
                                        "UnitOfMeasure": {
                                            "type": "string",
                                            "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                                        }
                                    },
                                    "additionalProperties": false
                                },
                                "CumulativePorosity": {
                                    "type": "object",
