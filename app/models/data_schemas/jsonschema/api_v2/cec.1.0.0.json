{
  "$id": "cec.1.0.0.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CecDataSchema",
  "description": "CecDataSchema",
  "type": "object",
  "properties": {
    "SamplesAnalysisID": {
      "type": "string",
      "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-SamplesAnalysis:[\\w\\-\\.\\:\\%]+:[0-9]*$",
      "x-osdu-relationship": [
        {
          "EntityType": "SamplesAnalysis",
          "GroupType": "work-product-component"
        }
      ]
    },
    "SampleID": {
      "type": "string",
      "pattern": "^[\\w\\-\\.]+:master-data\\-\\-Sample:[\\w\\-\\.\\:\\%]+:[0-9]*$",
      "x-osdu-relationship": [
        {
          "EntityType": "Sample",
          "GroupType": "master-data"
        }
      ]
    },
    "BrineConcentration": {
      "type": "object",
      "properties": {
        "Value": {
          "type": "number"
        },
        "UnitOfMeasure": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
          "x-osdu-relationship": [
            {
              "EntityType": "UnitOfMeasure",
              "GroupType": "reference-data"
            }
          ]
        }
      },
      "additionalProperties": false
    },
    "CeC": {
      "type": "object",
      "properties": {
        "Value": {
          "type": "number"
        },
        "UnitOfMeasure": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
          "x-osdu-relationship": [
            {
              "EntityType": "UnitOfMeasure",
              "GroupType": "reference-data"
            }
          ]
        }
      },
      "additionalProperties": false
    },
    "Qv": {
      "type": "object",
      "Description": "Contribution per unit pore volume",
      "properties": {
        "Value": {
          "type": "number"
        },
        "UnitOfMeasure": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
          "x-osdu-relationship": [
            {
              "EntityType": "UnitOfMeasure",
              "GroupType": "reference-data"
            }
          ]
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false,
  "required": [
    "SamplesAnalysisID",
    "SampleID"
  ]
}
