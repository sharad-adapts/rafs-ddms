{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "SamplesAnalysisID": {
      "type": "string",
      "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-SamplesAnalysis:[\\w\\-\\.\\:\\%]+:[0-9]*$"
    },
    "SampleID": {
      "type": "string",
      "pattern": "^[\\w\\-\\.]+:master-data\\-\\-Sample:[\\w\\-\\.\\:\\%]+:[0-9]*$"
    },
    "GrainDensity": {
      "type": "object",
      "properties": {
        "Value": {
          "type": "number"
        },
        "UnitOfMeasure": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        }
      },
      "additionalProperties": false
    },
    "BulkDensity": {
      "type": "object",
      "properties": {
        "Value": {
          "type": "number"
        },
        "UnitOfMeasure": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        }
      },
      "additionalProperties": false
    },
    "Measurements": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "SamplePreparation": {
              "type": "string",
              "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-SamplePreparationMethod:[\\w\\-\\.\\:\\%]+:[0-9]*$"
            },
            "Permeability": {
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "PermeabilityType": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-PermeabilityMeasurementType:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              },
              "additionalProperties": false
            },
            "Porosity": {
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "PorosityType": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-PorosityMeasurementType:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              },
              "additionalProperties": false
            },
            "GasSaturation": {
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              },
              "additionalProperties": false
            },
            "OilSaturation": {
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              },
              "additionalProperties": false
            },
            "WaterSaturation": {
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "SamplesAnalysisID",
    "SampleID"
  ]
}
