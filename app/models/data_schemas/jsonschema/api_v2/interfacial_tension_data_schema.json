{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
      "SamplesAnalysisID": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-SamplesAnalysis:[\\w\\-\\.\\:\\%]+:[0-9]*$"
      },
      "SampleID": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:master-data\\-\\-Sample:[\\w\\-\\.\\:\\%]+:[0-9]*$"
      },
      "TestNumber": {
          "title": "Test Number",
          "type": "string",
          "description": "A number for this test for purposes of e.g., tracking lab sequence."
      },
      "WettingPhase": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-WettabilityPhaseType:[\\w\\-\\.\\:\\%]+:[0-9]*$"
      },
      "NonWettingPhase": {
          "type": "string",
          "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-WettabilityPhaseType:[\\w\\-\\.\\:\\%]+:[0-9]*$"
      },
      "Surfactants": {
          "type": "array",
          "items": [
              {
                  "type": "object",
                  "properties": {
                      "ComponentName": {
                          "type": "string"
                      },
                      "ComponentMassFraction": {
                          "type": "object",
                          "properties": {
                              "Value": {
                                  "type": "number"
                              },
                              "UnitOfMeasure": {
                                  "type": "string",
                                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                              }
                          },
                          "additionalProperties": false
                      },
                      "ComponentMoleFraction": {
                          "type": "object",
                          "properties": {
                              "Value": {
                                  "type": "number"
                              },
                              "UnitOfMeasure": {
                                  "type": "string",
                                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                              }
                          },
                          "additionalProperties": false
                      }
                  },
                  "additionalProperties": false
              }
          ]
      },
      "Remark": {
          "title": "Remark",
          "type": "string"
      },
      "InterfacialTensionTestSteps": {
          "type": "array",
          "items": [
              {
                  "type": "object",
                  "properties": {
                      "StepNumber": {
                          "type": "string",
                          "title": "Step Number",
                          "description": "Internal step number which identifies the sequence of step in interfacial tension measurements"
                      },
                      "StepPressure": {
                          "type": "object",
                          "properties": {
                              "Value": {
                                  "type": "number"
                              },
                              "UnitOfMeasure": {
                                  "type": "string",
                                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                              }
                          },
                          "additionalProperties": false
                      },
                      "StepTemperature": {
                          "type": "object",
                          "properties": {
                              "Value": {
                                  "type": "number"
                              },
                              "UnitOfMeasure": {
                                  "type": "string",
                                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                              }
                          },
                          "additionalProperties": false
                      },
                      "WettingPhaseSaturation": {
                          "type": "number"
                      },
                      "SurfactantConcentration": {
                          "type": "object",
                          "properties": {
                              "Value": {
                                  "type": "number"
                              },
                              "UnitOfMeasure": {
                                  "type": "string",
                                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                              }
                          },
                          "additionalProperties": false
                      },
                      "InterfacialTension": {
                          "type": "object",
                          "properties": {
                              "Value": {
                                  "type": "number"
                              },
                              "UnitOfMeasure": {
                                  "type": "string",
                                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                              }
                          },
                          "additionalProperties": false
                      },
                      "Remark": {
                          "title": "Remark",
                          "type": "string"
                      }
                  },
                  "additionalProperties": false
              }
          ]
      }
  },
  "additionalProperties": false,
  "required": [
      "SamplesAnalysisID",
      "SampleID"
  ]
}
