{
  "$id": "multiple_salinity_tests.1.0.0.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "MultipleSalinityDataSchema",
  "description": "MultipleSalinityDataSchema",
  "type": "object",
  "properties": {
    "SamplesAnalysisID": {
      "type": "string",
      "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-SamplesAnalysis:[\\w\\-\\.\\:\\%]+:[0-9]*$",
      "x-osdu-relationship": [
        {
          "EntityType": "SamplesAnalysis",
          "GroupType": "work-product-component"
        }
      ]
    },
    "TestMethod": {
      "type": "string"
    },
    "SalinityTestResults": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "SampleID": {
            "type": "string",
            "pattern": "^[\\w\\-\\.]+:master-data\\-\\-Sample:[\\w\\-\\.\\:\\%]+:[0-9]*$",
            "x-osdu-relationship": [
              {
                "EntityType": "Sample",
                "GroupType": "master-data"
              }
            ]
          },
          "BQv": {
            "type": "object",
            "properties": {
              "Value": {
                "type": "number"
              },
              "UnitOfMeasure": {
                "type": "string",
                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
                "x-osdu-relationship": [
                  {
                    "EntityType": "UnitOfMeasure",
                    "GroupType": "reference-data"
                  }
                ]
              }
            },
            "additionalProperties": false
          },
          "Qv": {
            "type": "object",
            "properties": {
              "Value": {
                "type": "number"
              },
              "UnitOfMeasure": {
                "type": "string",
                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
                "x-osdu-relationship": [
                  {
                    "EntityType": "UnitOfMeasure",
                    "GroupType": "reference-data"
                  }
                ]
              }
            },
            "additionalProperties": false
          },
          "BrineResistivity": {
            "type": "object",
            "properties": {
              "Value": {
                "type": "number"
              },
              "UnitOfMeasure": {
                "type": "string",
                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
                "x-osdu-relationship": [
                  {
                    "EntityType": "UnitOfMeasure",
                    "GroupType": "reference-data"
                  }
                ]
              }
            },
            "additionalProperties": false
          },
          "AdjustedFormationResistivityFactor": {
            "type": "number"
          },
          "AdjustedSaturationExponent": {
            "type": "number"
          },
          "WaterSaturationOverResistivity": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "ResistivityIndex": {
                  "type": "number"
                },
                "WaterSaturation": {
                  "type": "object",
                  "properties": {
                    "Value": {
                      "type": "number"
                    },
                    "UnitOfMeasure": {
                      "type": "string",
                      "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
                      "x-osdu-relationship": [
                        {
                          "EntityType": "UnitOfMeasure",
                          "GroupType": "reference-data"
                        }
                      ]
                    }
                  },
                  "additionalProperties": false
                }
              },
              "additionalProperties": false
            }
          }
        },
        "additionalProperties": false
      }
    },
    "SalinityTestSteps": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "BrineSalinity": {
            "type": "object",
            "properties": {
              "Value": {
                "type": "number"
              },
              "UnitOfMeasure": {
                "type": "string",
                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
                "x-osdu-relationship": [
                  {
                    "EntityType": "UnitOfMeasure",
                    "GroupType": "reference-data"
                  }
                ]
              }
            },
            "additionalProperties": false
          },
          "BrineConductivity": {
            "type": "object",
            "properties": {
              "Value": {
                "type": "number"
              },
              "UnitOfMeasure": {
                "type": "string",
                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
                "x-osdu-relationship": [
                  {
                    "EntityType": "UnitOfMeasure",
                    "GroupType": "reference-data"
                  }
                ]
              }
            },
            "additionalProperties": false
          },
          "CoreConductivity": {
            "type": "object",
            "properties": {
              "Value": {
                "type": "number"
              },
              "UnitOfMeasure": {
                "type": "string",
                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
                "x-osdu-relationship": [
                  {
                    "EntityType": "UnitOfMeasure",
                    "GroupType": "reference-data"
                  }
                ]
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "SamplesAnalysisID"
  ]
}
