{
  "$id": "rock_compressibility.1.0.0.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RockCompressibilityDataSchema",
  "description": "RockCompressibilityDataSchema",
  "type": "object",
  "properties": {
    "SamplesAnalysisID": {
      "type": "string",
      "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-SamplesAnalysis:[\\w\\-\\.\\:\\%]+:[0-9]*$",
      "x-osdu-relationship": [
        {
          "EntityType": "SamplesAnalysis",
          "GroupType": "work-product-component"
        }
      ]
    },
    "SampleID": {
      "type": "string",
      "pattern": "^[\\w\\-\\.]+:master-data\\-\\-Sample:[\\w\\-\\.\\:\\%]+:[0-9]*$",
      "x-osdu-relationship": [
        {
          "EntityType": "Sample",
          "GroupType": "master-data"
        }
      ]
    },
    "CompressibilityTestSteps": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "StepNumber": {
            "type": "integer"
          },
          "ConfiningStress": {
            "type": "object",
            "properties": {
              "Value": {
                "type": "number"
              },
              "UnitOfMeasure": {
                "type": "string",
                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
                "x-osdu-relationship": [
                  {
                    "EntityType": "UnitOfMeasure",
                    "GroupType": "reference-data"
                  }
                ]
              }
            },
            "additionalProperties": false
          },
          "Porosity": {
            "type": "object",
            "properties": {
              "Value": {
                "type": "number"
              },
              "UnitOfMeasure": {
                "type": "string",
                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
                "x-osdu-relationship": [
                  {
                    "EntityType": "UnitOfMeasure",
                    "GroupType": "reference-data"
                  }
                ]
              }
            },
            "additionalProperties": false
          },
          "Compressibility": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "CompressibilityType": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-CompressibilityMeasurementType:[\\w\\-\\.\\:\\%]+:[0-9]*$",
                  "x-osdu-relationship": [
                    {
                      "EntityType": "CompressibilityMeasurementType",
                      "GroupType": "reference-data"
                    }
                  ]
                },
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$",
                  "x-osdu-relationship": [
                    {
                      "EntityType": "UnitOfMeasure",
                      "GroupType": "reference-data"
                    }
                  ]
                }
              },
              "additionalProperties": false
            }
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "SamplesAnalysisID",
    "SampleID"
  ]
}
