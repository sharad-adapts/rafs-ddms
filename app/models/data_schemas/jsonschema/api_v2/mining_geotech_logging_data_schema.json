{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "SamplesAnalysisID": {
        "type": "string",
        "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-SamplesAnalysis:[\\w\\-\\.\\:\\%]+:[0-9]*$"
      },
      "SampleID": {
        "type": "string",
        "pattern": "^[\\w\\-\\.]+:master-data\\-\\-Sample:[\\w\\-\\.\\:\\%]+:[0-9]*$"
      },
      "MiningGeotechData": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "GeolFrom": {
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              },
              "additionalProperties": false
            },
            "GeolTo": {
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              },
              "additionalProperties": false
            },
            "RecoveredLength": {
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              },
              "additionalProperties": false
            },
            "RQD": {
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              },
              "additionalProperties": false
            },
            "FractureCount": {
              "type": "object",
              "properties": {
                "Value": {
                  "type": "number"
                },
                "UnitOfMeasure": {
                  "type": "string",
                  "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
              },
              "additionalProperties": false
            },
            "Description": {
              "title": "Description",
              "type": "string",
              "description": "Description e.g., information."
            },
            "LoggedBy": {
              "title": "LoggedBy",
              "type": "string",
              "description": "Logged by."
            },
            "LogDate": {
              "format": "date-time",
              "title": "LogDate",
              "type": "string",
              "description": "LogDate e.g., 2021-01-01T00:00:00Z."
            }
          },
          "additionalProperties": false,
          "required": [
            "GeolFrom","GeolTo","RecoveredLength","RQD","FractureCount"
          ]
        }
      }
    },
    "additionalProperties": false,
    "required": [
      "SamplesAnalysisID",
      "SampleID"
    ]
  }
  