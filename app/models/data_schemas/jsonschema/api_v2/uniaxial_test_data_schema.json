{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "SamplesAnalysisID": {
            "type": "string",
            "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-SamplesAnalysis:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        },
        "data": {
            "type": "array",
            "items": {
                "types": "object",
                "properties": {
                    "SampleID": {
                        "type": "string",
                        "pattern": "^[\\w\\-\\.]+:master-data\\-\\-Sample:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                    },
                    "SampleOrientation": {
                        "title": "Sample Orientation",
                        "type": "string"
                    },
                    "LabSampleIdentifier": {
                        "title": "Laboratory Sample Identifier",
                        "type": "string"
                    },
                    "Remarks": {
                        "title": "Remarks",
                        "type": "string"
                    },
                    "DepletionCondition": {
                        "title": "Depletion Condition",
                        "type": "string"
                    },
                    "Lithology": {
                        "title": "Lithology",
                        "type": "string"
                    },
                    "Length": {
                        "type": "object",
                        "properties": {
                            "Value": {
                                "type": "number"
                            },
                            "UnitOfMeasure": {
                                "type": "string",
                                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                            }
                        }
                    },
                    "Diameter": {
                        "type": "object",
                        "properties": {
                            "Value": {
                                "type": "number"
                            },
                            "UnitOfMeasure": {
                                "type": "string",
                                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                            }
                        }
                    },
                    "Density": {
                        "type": "object",
                        "properties": {
                            "Value": {
                                "type": "number"
                            },
                            "UnitOfMeasure": {
                                "type": "string",
                                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                            }
                        }
                    },
                    "BulkCompressibility": {
                        "type": "object",
                        "properties": {
                            "Value": {
                                "type": "number"
                            },
                            "UnitOfMeasure": {
                                "type": "string",
                                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                            }
                        }
                    },
                    "PoreVolumeCompressibility": {
                        "type": "object",
                        "properties": {
                            "Value": {
                                "type": "number"
                            },
                            "UnitOfMeasure": {
                                "type": "string",
                                "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                            }
                        }
                    }
                }
            }
        }
    },
    "required": [
        "SamplesAnalysisID",
        "data"
    ]
}
