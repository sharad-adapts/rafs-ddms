{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "SamplesAnalysisID": {
            "type": "string",
            "pattern": "^[\\w\\-\\.]+:work-product-component\\-\\-SamplesAnalysis:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        },
        "SampleID": {
            "type": "string",
            "pattern": "^[\\w\\-\\.]+:master-data\\-\\-Sample:[\\w\\-\\.\\:\\%]+:[0-9]*$"
        },
        "WeightOfRockExtracted": {
            "type": "object",
            "description": "weight of the rock sample that has been submitted for extraction. note - may be a fraction of the whole rock sample",
            "properties": {
                "Value": {
                    "type": "number"
                },
                "UnitOfMeasure": {
                    "type": "string",
                    "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
            }
        },
        "WeightOfExtract": {
            "type": "object",
            "description": "weight of fluid extract obtained from parent rock sample",
            "properties": {
                "Value": {
                    "type": "number"
                },
                "UnitOfMeasure": {
                    "type": "string",
                    "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
            }
        },
        "ExtractByRockWeight": {
            "type": "object",
            "description": "extract yield: weight of extract / weight of parent rock sample",
            "properties": {
                "Value": {
                    "type": "number"
                },
                "UnitOfMeasure": {
                    "type": "string",
                    "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
            }
        },
        "ExtractOfTotalOrganicCarbon": {
            "type": "object",
            "description": "weight of extract as a percentage of Total Organic Carbon",
            "properties": {
                "Value": {
                    "type": "number"
                },
                "UnitOfMeasure": {
                    "type": "string",
                    "pattern": "^[\\w\\-\\.]+:reference-data\\-\\-UnitOfMeasure:[\\w\\-\\.\\:\\%]+:[0-9]*$"
                }
            }
        }
    },
    "required": [
        "SamplesAnalysisID",
        "SampleID"
    ]
  }
  
